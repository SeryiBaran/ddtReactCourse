{
  "name": "ddtreactcourse",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "alllint": "eslint ./src/**/* && stylelint ./src/**/*.css",
    "alllint:fix": "eslint ./src/**/* --fix && stylelint ./src/**/*.css --fix",
    "jslint": "eslint ./src/**/*",
    "csslint": "stylelint ./src/**/*.css",
    "jslint:fix": "eslint ./src/**/* --fix",
    "csslint:fix": "stylelint ./src/**/*.css --fix",
    "brs": "browser-sync start --server \"dist\" --files \"**/*\"",
    "format": "prettier --write ./src/**/*",
    "csssort": "postcss ./src/**/*.css --replace --config package.json --no-map",
    "cssprefix": "postcss ./dist/**/*.css --replace --use autoprefixer --config package.json --no-map",
    "prod": "npm run csssort && npm run build && npm run cssprefix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tanstack/react-location": "^3.7.4",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "browser-sync": "^2.27.9",
    "css-declaration-sorter": "^6.2.2",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "postcss": {
    "map": false,
    "plugins": {
      "css-declaration-sorter": {
        "order": "smacss"
      }
    }
  }
}
