{
  "name": "ddtreactcourse",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "alllint": "eslint ./src/**/* && stylelint ./src/**/*.css",
    "alllint:fix": "eslint ./src/**/* --fix && stylelint ./src/**/*.css --fix",
    "jslint": "eslint ./src/**/*",
    "csslint": "stylelint ./src/**/*.css",
    "jslint:fix": "eslint ./src/**/* --fix",
    "csslint:fix": "stylelint ./src/**/*.css --fix",
    "brs": "browser-sync start --server \"dist\" --files \"**/*\"",
    "format": "prettier --write ./src/**/*",
    "csssort": "postcss ./src/**/*.css --replace --config package.json",
    "cssprefix": "postcss ./dist/**/*.css --use autoprefixer --config package.json",
    "prod": "npm run csssort && npm run build && npm run cssprefix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "css-declaration-sorter": "^6.1.4",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "postcss": {
    "map": false,
    "plugins": {
      "css-declaration-sorter": {
        "order": "smacss"
      }
    }
  }
}
